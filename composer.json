{
    "name": "fas/di",
    "type": "library",
    "description": "Fast and simple dependency injection",
    "keywords": [
        "di",
        "container",
        "autowire"
    ],
    "homepage": "https://github.com/gielfeldt/fas-di",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Gielfeldt",
            "email": "thomas@gielfeldt.dk"
        }
    ],
    "provide": {
        "psr/container-implementation": "^2.0"
    },
    "require": {
        "php": ">=7.4.0",
        "psr/container": "^1.0 || ^2.0",
        "psr/log": "^1.1",
        "opis/closure": "^3.6",
        "ocramius/proxy-manager": "^2.11"
    },
    "autoload": {
        "psr-4": {
            "Fas\\DI\\": "src/DI/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "phpmd/phpmd": "^2.10",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload-dev": {
        "psr-4": {
            "Fas\\DI\\Tests\\": "tests/DI/"
        }
    }
}
